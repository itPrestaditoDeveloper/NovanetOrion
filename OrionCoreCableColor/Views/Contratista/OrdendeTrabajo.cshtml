             $.ajax({
                url: "@Url.Action("ModalDetalleInstalacion", "Contratista")",
                method: "get",
                 data: { idSolicitudInstalacion: idsolicitudinstalacion},
                success: function (resp) {
                    ShowModalBigScroll(resp);
                },
                error: function (resp) {
                    AlertaError();
                }
            });
        }


